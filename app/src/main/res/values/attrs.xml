<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- CTitle -->
    <declare-styleable name="CTitle">
        <attr name="titleValue" format="string" />
        <attr name="rightNote" format="string" />
        <attr name="leftBtn_visibility" format="boolean" />
        <attr name="left_btn_drawable" format="reference" />  <!-- 给button设置 图片 -->
        <attr name="display_left_back_drawable" format="boolean" />
        <attr name="right_text_margin_right" format="dimension" />
        <attr name="right_text_text_appearance" format="reference" />
        <attr name="rightNote_drawable" format="reference" />
        <attr name="title_right_btn_bg" format="reference" />
        <attr name="title_right_left_btn_bg" format="reference" />
        <attr name="title_left_btn_bg" format="reference" />
        <attr name="title_back" format="boolean" />
        <attr name="right_btn_margin_right" format="dimension" />
    </declare-styleable>

    <declare-styleable name="custom">
        <attr name="lable" format="string" />
        <attr name="errNote" format="string" />
        <attr name="hit" format="string" />
        <attr name="input_typed" format="string" />
        <attr name="length" format="integer" />
        <attr name="nomal_bg" format="reference" />
        <attr name="select_bg" format="reference" />
        <attr name="clear" format="boolean" />
        <attr name="text_color" format="reference" />

        <attr name="right_btn" format="reference" />  <!-- 给button设置 图片 -->
        <attr name="right_btn_text" format="reference" /><!-- 给button设置 字 -->

        <attr name="right_btn_bg" format="reference" /><!-- 给右边设置 图片 -->
        <attr name="left_btn_bg" format="reference" /><!-- 给左边设置 图片 -->

        <attr name="right_iamge_margin_right" format="dimension" /><!-- 右边图片的右边距 -->
        <attr name="right_iamge_width" format="dimension" /><!-- 右边图片的宽 -->
        <attr name="right_iamge_height" format="dimension" /><!-- 右边图片的高 -->

        <attr name="text_padding_left" format="dimension" /><!-- 设置文本左间距 -->

        <attr name="right_btn_width" format="dimension" /><!-- 右边按钮的宽 -->
        <attr name="right_btn_height" format="dimension" /><!-- 右边按钮宽高 -->
        <attr name="right_btn_padding_left" format="dimension" /><!-- 右边按钮左间距 -->
        <attr name="right_btn_padding_right" format="dimension" /><!-- 右边按钮右间距 -->
        <attr name="right_btn_text_appearance" format="reference" /><!-- 右边按钮文本样式 -->

    </declare-styleable>

    <!-- MineItemView我的页面单个条目控件 -->
    <declare-styleable name="MineItemView">
        <attr name="itemIcon" format="reference" />
        <attr name="itemText" format="string" />
        <attr name="itemOtherText" format="string" />
        <attr name="itemRightImg" format="reference" />
        <attr name="imgVisable" format="boolean" />
        <attr name="otherTextVisable" format="boolean" />
    </declare-styleable>

    <!-- RoundImageView圆形图片 -->
    <!--<declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>-->

</resources>